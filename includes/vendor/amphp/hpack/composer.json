{
    "name": "amphp/hpack",
    "homepage": "https://github.com/amphp/hpack",
    "description": "HTTP/2 HPack implementation.",
    "keywords": [
        "http-2",
        "hpack",
        "headers"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Lowrey",
            "email": "rdlowrey@php.net"
        },
        {
            "name": "Bob Weinand"
        },
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        },
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        }
    ],
    "require": {
